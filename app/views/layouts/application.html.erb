<!DOCTYPE html>
<html>
<head>
  <title>Artdealer</title>
  <%= stylesheet_link_tag "screen" %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="container">
    <div class="loginbox">
      <%= render :partial => "layouts/login_box" %>
    </div>
    <% if current_user %>
    <div class="navbox">
      <%= render :partial => "layouts/nav_box" %>
    </div>
    <% end %>
  </div>
  <%= yield %>

</body>
</html>
